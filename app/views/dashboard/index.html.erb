<div class="container">
	<div class="row">
		<div class="col">
			<h2>No Service Contract</h2>
			<ul>
			<% @no_support.each do |i| %>
				<li><%= i.summary %></li>
			<% end %>
			</ul>
		</div>
		<div class="col">
			<h2>Upcoming Expirations</h2>
			<ul>
			<% @expiring_support.each do |i| %>
				<li><%= i.summary %></li>
			<% end %>
			</ul>
		</div>
		<div class="col">
			<h2>Active Service Contract</h2>
			<ul>
			<% @active_support.each do |i| %>
				<li><%= i.summary %></li>
			<% end %>
			</ul>
		</div>
	</div>
</div>
